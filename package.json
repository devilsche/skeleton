{
    "name": "strato-skeleton",
    "displayName": "STRATO Skeleton (Control Panel)",
    "description": "Skeleton Template Language Support",
    "author": "Dominic Ruschinski",
    "license": "MIT",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.7.7"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "skeleton",
                "aliases": [ "STRATO Skeleton (Control Panel)" ],
                "extensions": [ ".html" ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "skeleton",
                "scopeName": "source.skeleton",
                "path": "./syntaxes/skeleton.tmLanguage.json"
            }
        ]
    }
}
